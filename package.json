{
  "scripts": {
    "build-dll": "(webpack -p --config webpack.dll.js --env.min && webpack --config webpack.dll.js) || true",
    "build-todolist": "(webpack -p --env.app=todolist --env.min && webpack --env.app=todolist) || true",
    "watch-todolist": "webpack --env.app=todolist --watch",
    "build-theme": "(sass-lint -v -q && node-sass --recursive --output moodle/theme/plus/style moodle/theme/plus/sass) || true",
    "watch-theme": "watch 'npm run build-theme' moodle/theme/plus/sass",
    "lint-theme": "sass-lint -v -q || true",
    "build-all": "npm-run-all build-dll build-todolist build-theme"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "6.10.3",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "sass-lint": "1.10.2",
    "watch": "1.0.2",
    "webpack": "2.5.0"
  },
  "dependencies": {
    "clone": "2.1.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "superagent": "3.5.2"
  }
}
