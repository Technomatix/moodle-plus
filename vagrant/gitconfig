[color]
    ui = true
[gui]
    editor = vim
[core]
    filemode = false
[alias]
    st = status
    br = branch -vv
    lg = log --graph --decorate --pretty=oneline --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    ss = show --stat
    co = checkout
